<!-- Team Member Profiles -->
<div class="container" id='bikes'>
      <div class="row">


      	<% brands =
      		{ :Cannondale =>
      			{
      				:image => 'http://www.austintricyclist.com/dir/2764/images/site/cannondale_logo_gn.jpg',
      				:url => '',
      				:name => 'Cannondale',
      				:decription => 'Best Bikes'
      			},
            :Specialized =>
            {
              :image => 'http://www.austintricyclist.com/dir/2764/images/site/cannondale_logo_gn.jpg',
              :url => '',
              :name => 'Specialized',
