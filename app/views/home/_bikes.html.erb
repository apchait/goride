        <div class="col-lg-12">
          <h2 class="page-header">Bikes</h2>
        </div>

      	<% brands.each do |name, brand| %>

      	<div class="col-sm-4">
        	<a href= "<%= brand[:url] %>"><%= image_tag brand[:image], :width => '150', :class => "img-responsive" %></a>
            <h3><%= brand[:name] %> <small>Subtitle</small></h3>
            <p><%= brand[:description] %></p>
            <ul class="list-unstyled list-inline list-social-icons">
              <li class="tooltip-social facebook-link"><a href="#facebook-profile" data-toggle="tooltip" data-placement="top" title="Facebook"><i class="icon-facebook-sign icon-2x"></i></a></li>
              <li class="tooltip-social linkedin-link"><a href="#linkedin-profile" data-toggle="tooltip" data-placement="top" title="LinkedIn"><i class="icon-linkedin-sign icon-2x"></i></a></li>
              <li class="tooltip-social twitter-link"><a href="#twitter-profile" data-toggle="tooltip" data-placement="top" title="Twitter"><i class="icon-twitter-sign icon-2x"></i></a></li>
              <li class="tooltip-social google-plus-link"><a href="#google-plus-profile" data-toggle="tooltip" data-placement="top" title="Google+"><i class="icon-google-plus-sign icon-2x"></i></a></li>
            </ul>
        </div>
        <% end %>

